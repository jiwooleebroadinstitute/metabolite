(cor.test(pad$b, vte$b)$estimate)^2
cor.test(pad$b, vte$b)$p.value

cad$outcome = "CAD"
pad$outcome = "PAD"
vte$outcome = "VTE"

total = rbind(rbind(cad, pad), vte)
total = total[which(total$method == "Inverse variance weighted"),]

library("gplots")
