# DEFINE FUNCTIONS
convert_camel_case <- function(x) {
  string_vec <- x
  # substitute underscores into spaces
  string_vec <- gsub(" ", "_", string_vec)
  # remove apostrophes
  string_vec <- gsub("\\'", "", string_vec)
  # remove commas
  string_vec <- gsub(",", "", string_vec)
  # remove dashes
  string_vec <- gsub("\\-", "_", string_vec)
  # remove \
  string_vec <- gsub("/", "_", string_vec)
  # convert lowercase
